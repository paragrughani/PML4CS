   "outputs": [],
   "source": [
    "#Multiply (Matrix Multiplication) M1 and M2 and store the results in M3, print M3\n",
    "\n",
    "##Write your code below this##\n",
    "\n",
    m3 = np.matmul(m1,m2)
    print("Multiplication of two matrix:",m3)
    print(m3.shape)
    "\n",
    "##Write your code above this##"
   ]
